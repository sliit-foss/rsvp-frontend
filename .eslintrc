{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["next", "next/core-web-vitals", "eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "rules": {
    "no-eval": "error",
    "no-unused-vars": "error",
    "no-unused-expressions": "error"
  }
}
